local TrollloSection = Trolllo:NewSection("Пвп")
TrollloSection:NewButton("Бить без задержки (почти) КНОПКА E", "Пвп", function()


local UserInputService = game:GetService("UserInputService")


local function onInputBegan(input, gameProcessedEvent)

    if not gameProcessedEvent then

        if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.E then
         game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("HitRequest"):InvokeServer()
        end
    end
end


UserInputService.InputBegan:Connect(onInputBegan)
end)

TrollloSection:NewButton("Cбор оружие КНОПКА Q", "Пвп", function()
local UIS = game:GetService("UserInputService")  
local Player = game.Players.LocalPlayer  


local function scanFolders(parent)  
    for _, child in ipairs(parent:GetChildren()) do  
        if child:IsA("Folder") and not child:FindFirstChild("zabor") then  
    
            local hitbox = child:FindFirstChild("Hitbox")  
            local handle = child:FindFirstChild("Handle")  
            local targetPart  

            if hitbox and hitbox:FindFirstChildOfClass("TouchTransmitter") then  
                targetPart = hitbox  
            elseif handle and handle:FindFirstChildOfClass("TouchTransmitter") then  
                targetPart = handle  
            end  


            if targetPart and Player.Character then  
                local root = Player.Character:FindFirstChild("HumanoidRootPart")  
                if root then  
                    firetouchinterest(root, targetPart, 0)  
                    firetouchinterest(root, targetPart, 1)  

                end  
            end  


            scanFolders(child)  
        end  
    end  
end  

-- Бинд на Q  
UIS.InputBegan:Connect(function(input)  
    if input.KeyCode == Enum.KeyCode.Q then  
        scanFolders(workspace) 

    end  
end)  
end)
local TrollloSection = Trolllo:NewSection("Другое")
TrollloSection:NewButton("Удалить бананы (АВТО)", "Тест", function()
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")


local function delayedDestroy(part)
    if part and part.Parent then

        task.delay(0.001, function()
            if part and part.Parent then
                part:Destroy()
            end
        end)
    end
end


for _, descendant in ipairs(Workspace:GetDescendants()) do
    if descendant:IsA("BasePart") and descendant.Name == "Banana" then
        delayedDestroy(descendant)
    end
end


Workspace.DescendantAdded:Connect(function(descendant)
    if descendant:IsA("BasePart") and descendant.Name == "Banana" then
        delayedDestroy(descendant)
    end
end)


local function fastCleanup()
    for _, part in ipairs(Workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.Name == "Banana" then
            delayedDestroy(part)
        end
    end
end


while true do
    task.wait(0.5)
    fastCleanup()
end
end)

TrollloSection:NewButton("Удалить опасные объекты и рэгдоллы", "Тест", function()
local Workspace = game:GetService("Workspace")


local foldersToRemove = {
    "FireParts",
    "KillParts",
    "PoisonParts",
    "RagdollParts"
}


for _, folderName in ipairs(foldersToRemove) do
    local folder = Workspace:FindFirstChild(folderName)
    if folder then
        folder:Destroy()

    else

    end
end


end)

local DolbaebBrawl = Window:NewTab("BrawlStarsDolbaeb417")
