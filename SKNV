-- LocalScript

local UserInputService = game:GetService("UserInputService")

-- –§—É–Ω–∫—Ü–∏—è, –∫–æ—Ç–æ—Ä–∞—è –±—É–¥–µ—Ç –≤—ã–∑—ã–≤–∞—Ç—å—Å—è –ø—Ä–∏ –Ω–∞–∂–∞—Ç–∏–∏ –∫–ª–∞–≤–∏—à–∏
local function onInputBegan(input, gameProcessedEvent)
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ —Å–æ–±—ã—Ç–∏–µ –Ω–µ –æ–±—Ä–∞–±–æ—Ç–∞–Ω–æ –∏–≥—Ä–æ–π (–Ω–∞–ø—Ä–∏–º–µ—Ä, –Ω–µ –æ—Ç–∫—Ä—ã—Ç–æ –º–µ–Ω—é)
    if not gameProcessedEvent then
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –±—ã–ª–∞ –ª–∏ –Ω–∞–∂–∞—Ç–∞ –∫–ª–∞–≤–∏—à–∞ "E"
        if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.E then
         game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("HitRequest"):InvokeServer()
        end
    end
end

-- –ü–æ–¥–ø–∏—Å—ã–≤–∞–µ–º—Å—è –Ω–∞ —Å–æ–±—ã—Ç–∏–µ –≤–≤–æ–¥–∞
UserInputService.InputBegan:Connect(onInputBegan)
end)

TrollloSection:NewButton("C–±–æ—Ä –æ—Ä—É–∂–∏–µ –ö–ù–û–ü–ö–ê Q", "–ü–≤–ø", function()
local UIS = game:GetService("UserInputService")  
local Player = game.Players.LocalPlayer  

-- –†–µ–∫—É—Ä—Å–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫ –ø–∞–ø–æ–∫ —Å –∏–≥–Ω–æ—Ä–æ–º "zabor"  
local function scanFolders(parent)  
    for _, child in ipairs(parent:GetChildren()) do  
        if child:IsA("Folder") and not child:FindFirstChild("zabor") then  
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º Hitbox/Handle –≤ —Ç–µ–∫—É—â–µ–π –ø–∞–ø–∫–µ  
            local hitbox = child:FindFirstChild("Hitbox")  
            local handle = child:FindFirstChild("Handle")  
            local targetPart  

            if hitbox and hitbox:FindFirstChildOfClass("TouchTransmitter") then  
                targetPart = hitbox  
            elseif handle and handle:FindFirstChildOfClass("TouchTransmitter") then  
                targetPart = handle  
            end  

            -- –ê–∫—Ç–∏–≤–∞—Ü–∏—è, –µ—Å–ª–∏ –Ω–∞–π–¥–µ–Ω–∞ —Ü–µ–ª—å  
            if targetPart and Player.Character then  
                local root = Player.Character:FindFirstChild("HumanoidRootPart")  
                if root then  
                    firetouchinterest(root, targetPart, 0)  
                    firetouchinterest(root, targetPart, 1)  
                    print("‚ö° –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–æ –≤ –ø–∞–ø–∫–µ:", child:GetFullName())  
                end  
            end  

            -- –†–µ–∫—É—Ä—Å–∏–≤–Ω–æ –ø—Ä–æ–≤–µ—Ä—è–µ–º –≤–ª–æ–∂–µ–Ω–Ω—ã–µ –ø–∞–ø–∫–∏  
            scanFolders(child)  
        end  
    end  
end  

-- –ë–∏–Ω–¥ –Ω–∞ Q  
UIS.InputBegan:Connect(function(input)  
    if input.KeyCode == Enum.KeyCode.Q then  
        scanFolders(workspace) -- –°–∫–∞–Ω–∏—Ä—É–µ–º –≤–µ—Å—å Workspace  
        print("üîç –ü—Ä–æ—Å–∫–∞–Ω–∏—Ä–æ–≤–∞–Ω–æ!")  
    end  
end)  
end)
local TrollloSection = Trolllo:NewSection("–î—Ä—É–≥–æ–µ")
TrollloSection:NewButton("–£–¥–∞–ª–∏—Ç—å –±–∞–Ω–∞–Ω—ã (–ê–í–¢–û)", "–¢–µ—Å—Ç", function()
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ª–æ–∂–µ–Ω–Ω–æ–≥–æ —É–¥–∞–ª–µ–Ω–∏—è
local function delayedDestroy(part)
    if part and part.Parent then
        -- –ñ–¥–µ–º 1 –º–∏–ª–ª–∏—Å–µ–∫—É–Ω–¥—É –ø–µ—Ä–µ–¥ —É–¥–∞–ª–µ–Ω–∏–µ–º
        task.delay(0.001, function()
            if part and part.Parent then
                part:Destroy()
            end
        end)
    end
end

-- –£–¥–∞–ª–µ–Ω–∏–µ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö Banana-—á–∞—Å—Ç–µ–π
for _, descendant in ipairs(Workspace:GetDescendants()) do
    if descendant:IsA("BasePart") and descendant.Name == "Banana" then
        delayedDestroy(descendant)
    end
end

-- –û—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –Ω–æ–≤—ã—Ö Banana-—á–∞—Å—Ç–µ–π
Workspace.DescendantAdded:Connect(function(descendant)
    if descendant:IsA("BasePart") and descendant.Name == "Banana" then
        delayedDestroy(descendant)
    end
end)

-- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∑–∞—â–∏—Ç–∞ –¥–ª—è –±—ã—Å—Ç—Ä–æ —Å–æ–∑–¥–∞–≤–∞–µ–º—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
local function fastCleanup()
    for _, part in ipairs(Workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.Name == "Banana" then
            delayedDestroy(part)
        end
    end
end

-- –†–µ–≥—É–ª—è—Ä–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –∫–∞–∂–¥—ã–µ 0.5 —Å–µ–∫—É–Ω–¥
while true do
    task.wait(0.5)
    fastCleanup()
end
end)

TrollloSection:NewButton("–£–¥–∞–ª–∏—Ç—å –æ–ø–∞—Å–Ω—ã–µ –æ–±—ä–µ–∫—Ç—ã –∏ —Ä—ç–≥–¥–æ–ª–ª—ã", "–¢–µ—Å—Ç", function()
local Workspace = game:GetService("Workspace")

-- –°–ø–∏—Å–æ–∫ –ø–∞–ø–æ–∫ –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è
local foldersToRemove = {
    "FireParts",
    "KillParts",
    "PoisonParts",
    "RagdollParts"
}

-- –£–¥–∞–ª—è–µ–º –≤—Å–µ —É–∫–∞–∑–∞–Ω–Ω—ã–µ –ø–∞–ø–∫–∏ –æ–¥–Ω–æ–∫—Ä–∞—Ç–Ω–æ –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
for _, folderName in ipairs(foldersToRemove) do
    local folder = Workspace:FindFirstChild(folderName)
    if folder then
        folder:Destroy()
        print("–£–¥–∞–ª–µ–Ω–∞ –ø–∞–ø–∫–∞: " .. folderName)
    else
        print("–ü–∞–ø–∫–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω–∞: " .. folderName)
    end
end

print("–û—á–∏—Å—Ç–∫–∞ –ø–∞–ø–æ–∫ –∑–∞–≤–µ—Ä—à–µ–Ω–∞")
end)

