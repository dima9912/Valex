xyiSection:NewButton("Сдохнуть (Быстрый)", "lol", function()
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Respawn"):InvokeServer()
end)

xyiSection:NewButton("ПОНОС", "lol", function()
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")


local function activatePoisonEffect()
    local player = Players.LocalPlayer
    if not player or not player.Character then return end
    
    local hrp = player.Character:FindFirstChild("HumanoidRootPart")
    local humanoid = player.Character:FindFirstChild("Humanoid")
    local poisonPart = workspace:FindFirstChild("PoisonParts") and workspace.PoisonParts:FindFirstChild("Union")
    

    if not hrp or not humanoid or not poisonPart or humanoid.Health < 1 then
        return false
    end
    

    firetouchinterest(poisonPart, hrp, 0) 
    RunService.Heartbeat:Wait() 
    firetouchinterest(poisonPart, hrp, 1)
    
    return true
end


while task.wait(0.1) do
    if not activatePoisonEffect() then
        break 
    end
end
end)

